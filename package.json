{
  "name": "vcard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 3042",
    "build": "next build",
    "start": "next start",
    "export": "NODE_ENV=production next build && next export",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:js",
    "lint-fix": "npm run lint:css-fix && npm run lint:js-fix",
    "lint:css": "stylelint \"**/*.js\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint:js-fix": "eslint \"**/*.js\" --fix",
    "lint:html": "htmlhint out",
    "minify-html": "html-minifier --input-dir out --output-dir out --file-ext html --minify-css",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json static/assets/images/favicons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schoenwaldnils/vcard.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/schoenwaldnils/vcard/issues"
  },
  "homepage": "https://github.com/schoenwaldnils/vcard#readme",
  "dependencies": {
    "@emotion/cache": "10.0.19",
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "babel-plugin-inline-react-svg": "1.1.0",
    "cli-real-favicon": "0.0.8",
    "emotion-theming": "10.0.14",
    "file-loader": "4.2.0",
    "html-minifier": "^4.0.0",
    "html-react-parser": "^0.9.1",
    "next": "^9.0.4",
    "prop-types": "^15.7.2",
    "raw-loader": "3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-icons": "3.7.0",
    "url-loader": "2.1.0",
    "webpack-filter-warnings-plugin": "1.2.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-schoenwaldnils": "3.0.1",
    "htmlhint": "^0.11.0",
    "husky": "^3.0.4",
    "lint-staged": "9.4.2",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-schoenwaldnils": "^2.1.1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "stylelint --formatter verbose",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
